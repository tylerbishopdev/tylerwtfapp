                  "HALFTONE_PRINT",
                  "HIGH_CONTRAST",
                  "HIPPIE_ERA",
                  "ICONIC",
                  "JAPANDI_FUSION",
                  "JAZZY",
                  "LONG_EXPOSURE",
                  "MAGAZINE_EDITORIAL",
