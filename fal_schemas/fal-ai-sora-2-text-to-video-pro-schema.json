        "title": "ProTextToVideoOutput",
        "type": "object",
        "properties": {
          "video": {
            "examples": [
              {
                "content_type": "video/mp4",
                "url": "https://storage.googleapis.com/falserverless/example_outputs/sora-2-pro-t2v-output.mp4"
              }
            ],
            "title": "Video",
            "description": "The generated video",
            "allOf": [
